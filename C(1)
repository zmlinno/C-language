int main()
{
	int line = 0;

	while (line<=20000)
	{
		printf("敲代码: %d\n", line);
		line++;
	}

	if (line > 20000)
		printf("好offer\n");
	else
		printf("好好学习，天天编程\n");

	return 0;
}
